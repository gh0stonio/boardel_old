{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "#components": "./src/components",
          "#hooks": "./src/hooks",
          "#utils": "./src/utils",
          "#store": "./src/store",
          "#constants": "./src/constants"
        }
      }
    ]
  ]
}
